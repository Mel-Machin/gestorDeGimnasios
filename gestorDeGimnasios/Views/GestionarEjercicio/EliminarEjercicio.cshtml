@{
}
@model gestorDeGimnasios.Models.Ejercicio


<form asp-action="AccionEliminarEjercicio" method="post">
    <div class="form-group">
        @Html.LabelFor(model => model.IdEjercicio, htmlAttributes: new { @class = "control-label" })
        @Html.TextBoxFor(model => model.IdEjercicio, new { @class = "form-control", @readonly = "readonly" })
    </div>

    <div>
        @Html.LabelFor(model => model.Descripcion, htmlAttributes: new { @class = "control-label" })
        @Html.EditorFor(model => model.Descripcion, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.IdTipoMaquina, htmlAttributes: new { @class = "control-label" })
        @Html.EditorFor(model => model.IdTipoMaquina, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </div>

    <input type="submit" value="Eliminar" class="btn btn-primary" />
</form>