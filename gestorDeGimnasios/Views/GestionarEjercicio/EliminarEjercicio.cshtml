@{
}
@model gestorDeGimnasios.Models.Ejercicio

<h2 class="mainTitulo">Confirmar Eliminación de Ejercicio</h2>

<div class="mainContenido">
<form asp-action="AccionEliminarEjercicio" method="post" class="formulario">

    @Html.LabelFor(model => model.IdEjercicio, htmlAttributes: new { @class = "label" })
    @Html.TextBoxFor(model => model.IdEjercicio, new { @class = "input", @readonly = "readonly" })
    <div></div>


    @Html.LabelFor(model => model.Descripcion, htmlAttributes: new { @class = "label" })
    @Html.TextBoxFor(model => model.Descripcion, new { @class = "input", @readonly = "readonly" })
    <div></div>


    @Html.LabelFor(model => model.IdTipoMaquina, htmlAttributes: new { @class = "label" })
    @Html.TextBoxFor(model => model.IdTipoMaquina, new { @class = "input", @readonly = "readonly" })
    <div></div>

    <input type="submit" value="Eliminar" class="boton" />
</form>
</div>