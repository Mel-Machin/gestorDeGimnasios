@{
}
<h2>
    Administrar Ejercicios
</h2>
@Html.ActionLink("Registrar", "RegistrarEjercicio")
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Descripción</th>
            <th>Id tipo de maquina</th>
            <th>Rutinas Relacionadas</th>
        </tr>

    </thead>
    <tbody>
        @foreach (Ejercicio ejercicio in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => ejercicio.IdEjercicio)</td>
                <td>@Html.DisplayFor(modelItem => ejercicio.Descripcion)</td>
                <td>@Html.DisplayFor(modelItem => ejercicio.IdTipoMaquina)</td>
                <td> 
                    @Html.ActionLink("Editar", "EditarEjercicio", new { idEjercicio = ejercicio.IdEjercicio })
                    @Html.ActionLink("Eliminar", "EliminarEjercicio", new { idEjercicio = ejercicio.IdEjercicio })
                </td>
            </tr>
        }
    </tbody>
</table>
