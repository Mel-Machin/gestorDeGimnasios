@{
}
@model gestorDeGimnasios.Models.Ejercicio


<form asp-action="AccionRegistrarEjercicio" method="post">
  
    <div class="form-group">
        @Html.LabelFor(model => model.Descripcion, htmlAttributes: new { @class = "control-label" })
        @Html.TextBoxFor(model => model.IdTipoMaquina, new { @class = "form-control", type = "text" })
        @Html.ValidationMessageFor(model => model.IdTipoMaquina, "", new { @class = "text-danger" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.IdTipoMaquina, htmlAttributes: new { @class = "control-label" })
        @Html.TextBoxFor(model => model.IdTipoMaquina, new { @class = "form-control", type = "number" })
        @Html.ValidationMessageFor(model => model.IdTipoMaquina, "", new { @class = "text-danger" })
    </div>

    <input type="submit" value="Agregar" class="btn btn-primary" />

</form>
