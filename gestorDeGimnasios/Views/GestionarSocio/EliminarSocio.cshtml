@{
}
@model gestorDeGimnasios.Models.Socio


<h2 class="mainTitulo">Confirmar Eliminación del Socio</h2>
<div class="mainContenido">
    <form asp-action="AccionEliminarSocio" method="post" class="formulario">

        @Html.LabelFor(model => model.IdSocio, htmlAttributes: new { @class = "label" })
        @Html.TextBoxFor(model => model.IdSocio, new { @class = "input", @readonly = "readonly" })
        <div></div>

        @Html.LabelFor(model => model.Nombre, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.Nombre, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>


        @Html.LabelFor(model => model.Apellido, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.Apellido, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>


        @Html.LabelFor(model => model.Tipo, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.Tipo, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>


        @Html.LabelFor(model => model.Telefono, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.Telefono, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>


        @Html.LabelFor(model => model.CorreoElectronico, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.CorreoElectronico, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>


        @Html.LabelFor(model => model.IdLocal, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.IdLocal, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        <input type="submit" value="Eliminar" class="boton" />
    </form>
</div>