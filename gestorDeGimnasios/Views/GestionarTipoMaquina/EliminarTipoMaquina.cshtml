@{
}

@model gestorDeGimnasios.Models.TipoMaquina


<form asp-action="AccionEliminarTipoMaquina" method="post">

    <div class="form-group">
        @Html.LabelFor(model => model.IdTipoMaquina, htmlAttributes: new { @class = "control-label" })
        @Html.TextBoxFor(model => model.IdTipoMaquina, new { @class = "form-control", type = "number", @readonly = "readonly" })
    </div>

    <div>
        @Html.LabelFor(model => model.Nombre, htmlAttributes: new { @class = "control-label" })
        @Html.EditorFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </div>

    <input type="submit" value="Eliminar" class="btn btn-primary" />
</form>
