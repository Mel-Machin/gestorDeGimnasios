@{
}
@model gestorDeGimnasios.Models.TipoMaquina


<form asp-action="AccionEditarTipoMaquina" method="post">

    <div class="form-group">
        @Html.LabelFor(model => model.IdTipoMaquina, htmlAttributes: new { @class = "control-label" })
        @Html.TextBoxFor(model => model.IdTipoMaquina, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </div>

    <div>
        @Html.LabelFor(model => model.Nombre, htmlAttributes: new { @class = "control-label" })
        @Html.EditorFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control", type = "text" } })
        @Html.ValidationMessageFor(model => model.Nombre, "", new { @class = "text-danger" })
    </div>

    <input type="submit" value="Guardar cambios" class="btn btn-primary" />
</form>