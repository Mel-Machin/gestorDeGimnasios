@{

}
@model gestorDeGimnasios.Models.Maquina

<h2 class="mainTitulo">Confirmar Eliminación de la Maquina</h2>

<div class="mainContenido">
    <form asp-action="AccionEliminarMaquina" method="post" class="formulario">

        @Html.LabelFor(model => model.IdMaquina, htmlAttributes: new { @class = "label" })
        @Html.TextBoxFor(model => model.IdMaquina, new { @class = "input", @readonly = "readonly" })
        <div></div>

        @Html.LabelFor(model => model.IdTipoMaquina, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.IdTipoMaquina, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        @Html.LabelFor(model => model.IdLocal, htmlAttributes: new { @class = "label" })
        @Html.TextBoxFor(model => model.IdLocal, new { @class = "input", @readonly = "readonly" })
        <div></div>

        @Html.LabelFor(model => model.FechaCompra, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.FechaCompra, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        @Html.LabelFor(model => model.Precio, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.Precio, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        @Html.LabelFor(model => model.VidaUtil, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.VidaUtil, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        @Html.LabelFor(model => model.Disponibilidad, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.Disponibilidad, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        <input type="submit" value="Eliminar" class="boton" />
    </form>
</div>