@{
}
@model gestorDeGimnasios.Models.Rutina

<h2 class="mainTitulo">Confirmar Eliminación de la Rutina.</h2>
<div class="mainContenido">
    <form asp-action="AccionEliminarRutina" method="post" class="formulario">


        @Html.LabelFor(model => model.IdRutina, htmlAttributes: new { @class = "label" })
        @Html.TextBoxFor(model => model.IdRutina, new { @class = "input", @readonly = "readonly" })
        <div></div>


        @Html.LabelFor(model => model.Descripcion, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.Descripcion, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        @Html.LabelFor(model => model.TipoRutina, htmlAttributes: new { @class = "label" })
        @Html.EditorFor(model => model.TipoRutina, new { htmlAttributes = new { @class = "input", @readonly = "readonly" } })
        <div></div>

        <input type="submit" value="Eliminar" class="boton" />
    </form>
</div>